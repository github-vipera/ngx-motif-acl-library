{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-motif-acl": {
      "root": "projects/ngx-motif-acl",
      "sourceRoot": "projects/ngx-motif-acl/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-motif-acl/tsconfig.lib.json",
            "project": "projects/ngx-motif-acl/ng-package.json"
          },
          "configurations": {
            "production" : {
              "project": "projects/ngx-motif-acl/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-motif-acl/src/test.ts",
            "tsConfig": "projects/ngx-motif-acl/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-motif-acl/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-motif-acl/tsconfig.lib.json",
              "projects/ngx-motif-acl/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-motif-acl-test-app": {
      "root": "projects/ngx-motif-acl-test-app/",
      "sourceRoot": "projects/ngx-motif-acl-test-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-motif-acl-test-app",
            "index": "projects/ngx-motif-acl-test-app/src/index.html",
            "main": "projects/ngx-motif-acl-test-app/src/main.ts",
            "polyfills": "projects/ngx-motif-acl-test-app/src/polyfills.ts",
            "tsConfig": "projects/ngx-motif-acl-test-app/tsconfig.app.json",
            "assets": [
              "projects/ngx-motif-acl-test-app/src/favicon.ico",
              "projects/ngx-motif-acl-test-app/src/assets"
            ],
            "styles": [
              "projects/ngx-motif-acl-test-app/src/styles.css",
              "projects/ngx-motif-acl-test-app/src/assets/motif_webadmin_iconfont.css",
              "projects/ngx-motif-acl-test-app/src/assets/style.css",
              "projects/ngx-motif-acl-test-app/src/assets/wc-uikit-kendo.css",
              "projects/ngx-motif-acl-test-app/src/assets/wc-uikit-toast.css",
              "projects/ngx-motif-acl-test-app/src/assets/wc-uikit-dashboard.css",
              "projects/ngx-motif-acl-test-app/src/assets/wc-uikit-primeng.css",

            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ngx-motif-acl-test-app/src/environments/environment.ts",
                  "with": "projects/ngx-motif-acl-test-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-motif-acl-test-app:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-motif-acl-test-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-motif-acl-test-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-motif-acl-test-app/src/test.ts",
            "polyfills": "projects/ngx-motif-acl-test-app/src/polyfills.ts",
            "tsConfig": "projects/ngx-motif-acl-test-app/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-motif-acl-test-app/karma.conf.js",
            "styles": [
              "projects/ngx-motif-acl-test-app/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/ngx-motif-acl-test-app/src/favicon.ico",
              "projects/ngx-motif-acl-test-app/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-motif-acl-test-app/tsconfig.app.json",
              "projects/ngx-motif-acl-test-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-motif-acl-test-app-e2e": {
      "root": "projects/ngx-motif-acl-test-app-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ngx-motif-acl-test-app-e2e/protractor.conf.js",
            "devServerTarget": "ngx-motif-acl-test-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-motif-acl-test-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/ngx-motif-acl-test-app-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-motif-acl"
}
